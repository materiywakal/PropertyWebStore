@model WebApp.ViewModels.BrowseFilterModel

<div>
    @Html.Label("Этаж")
    @Html.EditorFor(m => m.Floor)
</div>
<div>
    @Html.Label("Количество предлагаемых комнат")
    @Html.EditorFor(m => m.OfferingRoomAmount)
</div>
<div>
    @Html.Label("Проходная комната")
    @Html.RadioButtonFor(m => m.IsPassageRoom, true)
    @Html.Label("С отдельным входом")
    @Html.RadioButtonFor(m => m.IsPassageRoom, false)
</div>
<div>
    @Html.Label("Наличие мебели")
    @Html.CheckBoxFor(m => m.IsFurnitureExist, false)
</div>
<div>
    @Html.Label("Сдача посуточно")
    @Html.RadioButtonFor(m => m.IsOneDayRent, true)
    @Html.Label("На длительный срок")
    @Html.RadioButtonFor(m => m.IsOneDayRent, false)
</div>
                